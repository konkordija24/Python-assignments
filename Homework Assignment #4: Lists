Homework Assignment #4: Lists


myUniqueList = []
myLeftovers = []

def addToList(list, x):
	for n in list:
		if(list[n] != x):
			list.append(x)
			return True
		 else:
		 	myLeftovers.append(x)
		 	return False


addToList(myUniqueList, 4)
addToList(myUniqueList, 5)
addToList(myUniqueList, 8)
addToList(myUniqueList, 12)
print(myUniqueList)
addToList(myUniqueList, 12)
addToList(myUniqueList, 12)
addToList(myUniqueList, 156)
addToList(myUniqueList, 1256)
print(myUniqueList)
print(myLeftovers)
addToList(myUniqueList, 8)
addToList(myUniqueList, 8)
addToList(myUniqueList, 2454)
addToList(myUniqueList, 463)
addToList(myUniqueList, "sto se radi")
print(myUniqueList)
print(myLeftovers)
